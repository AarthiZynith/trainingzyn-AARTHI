pageextension 50126 ExtendedTextLines Extends "Extended Text Lines"
{
    layout{
        addafter(text){
            
            field(Bold;Rec.Bold){
                ApplicationArea=All;
            }
            field(Italics;Rec.Italics){
                ApplicationArea=All;
            }
            field(Underline;Rec.Underline){
                ApplicationArea=All;
            }
        }
    }
}